<?xml version="1.0" encoding="utf-8" standalone="no"?>
<widget appId="ea87b9af-aaf3-49af-90ce-2fc24f7c604c" version="1.0.0">
	<name>corMobi</name>
	<description/>
	<content encoding="utf-8" src="module/index.html" />
	<obfuscation>true</obfuscation>
	<bgcolor>#fefefe</bgcolor>
	<orientation>5</orientation>
	<debug enable="true" verbose="true">true</debug>
	<webapp/>
	<config desc="privacyConfig" type="AUTHORITY">
		<permission info="privacy" platform="iOS" type="camera">
			<string>拍照需要使用摄像头</string>
		</permission>
		<permission info="privacy" platform="iOS" type="photoLibrary">
			<string>选取图片需要访问相册</string>
		</permission>
		<permission info="privacy" platform="iOS" type="microphone">
			<string>录制语音需要使用麦克风</string>
		</permission>
		<permission info="privacy" platform="iOS" type="contact">
			<string>查找好友需要访问通讯录</string>
		</permission>
		<permission info="privacy" platform="iOS" type="locationWhenInUse">
			<string>需要访问你的位置信息</string>
		</permission>
		<permission info="privacy" platform="iOS" type="camera">
			<string>需要访问你的相机</string>
		</permission>
	</config>
	<config desc="backgroundConfig" type="AUTHORITY">
		<permission platform="iOS" info="backgroundMode" flag="16" />
	</config>
	<config desc="statusBar" type="STATUSBARSTYLE">
		<permission platform="iOS" info="statusBarStyle" value="Light" />
		<permission platform="iOS" info="statusBarStyleIOS7" value="1" />
	</config>
</widget>